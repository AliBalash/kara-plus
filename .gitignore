# Laravel specific files and folders
/vendor/
/node_modules/
.env

# Ignore storage except gitignore in the directory
/storage/*.key
/storage/*.log
/storage/*.cache
/storage/*.bak
/storage/framework/*
!/storage/framework/.gitignore
/storage/logs/*
!/storage/logs/.gitignore
/storage/app/public/*
!/storage/app/public/.gitignore

# Ignore IDE and editor files
.idea/
.vscode/
*.swp
*.swo

# Ignore Composer and NPM/Yarn related files
composer.lock
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore OS generated files
.DS_Store
Thumbs.db

# Ignore Laravel Mix related files
/public/hot
/public/storage
/public/mix-manifest.json

# Ignore testing related files
phpunit.result.cache

# Ignore other temporary files
*.bak
*.tmp
*.temp
